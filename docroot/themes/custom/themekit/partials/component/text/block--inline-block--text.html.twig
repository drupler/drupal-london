{% extends "@partials/component/block.html.twig" %}

{% set block = content['#block_content'] %}
{% set classes = [
  block.field_frame_background.value ? 'frame-background',
  block.field_frame_background.value ? 'frame-background--' ~ block.field_frame_background.value|clean_class,
] %}
{% set attributes = attributes.addClass(classes) %}

{% block content %}
  {{ content }}
{% endblock %}
